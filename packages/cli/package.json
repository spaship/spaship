{
  "name": "@spaship/cli",
  "description": "A command line interface for SPAship!",
  "version": "0.7.0",
  "author": "mclayton@redhat.com",
  "bin": {
    "spaship": "./bin/run"
  },
  "homepage": "https://github.com/spaship/spaship/tree/master/packages/cli#readme",
  "bugs": "https://github.com/spaship/spaship/issues",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:spaship/spaship.git",
    "directory": "packages/cli"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "preversion": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@spaship/common": "^0.7.0",
    "cosmiconfig": "^6.0.0",
    "execa": "^4.0.0",
    "inquirer": "^7.0.0",
    "lodash": "^4.17.15",
    "nconf": "^0.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@oclif/test": "1.2.5",
    "chai": "4.2.0",
    "eslint-config-oclif": "3.1.0",
    "globby": "11.0.0",
    "mocha": "7.1.1",
    "nock": "12.0.3",
    "nyc": "15.0.1",
    "shortid": "2.2.15",
    "temp-dir": "2.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "oclif": {
    "commands": "./src/commands",
    "bin": "spaship",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
