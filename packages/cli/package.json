{
  "name": "@spaship/cli",
  "description": "A command line interface for SPAship!",
  "version": "0.14.0",
  "author": "mclayton@redhat.com",
  "bin": {
    "spaship": "./bin/run"
  },
  "homepage": "https://github.com/spaship/spaship/tree/master/packages/cli#readme",
  "bugs": "https://github.com/spaship/spaship/issues",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:spaship/spaship.git",
    "directory": "packages/cli"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "preversion": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^5.0.0",
    "@spaship/common": "^0.14.0",
    "archiver": "^5.0.0",
    "cosmiconfig": "^7.0.0",
    "form-data": "^4.0.0",
    "inquirer": "^8.0.0",
    "lodash": "^4.17.15",
    "nconf": "^0.11.0",
    "ora": "^6.0.0",
    "pretty-bytes": "^5.3.0",
    "read-pkg-up": "^7.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.10",
    "@oclif/test": "2.0.3",
    "chai": "4.3.4",
    "eslint-config-oclif": "4.0.0",
    "globby": "12.2.0",
    "mocha": "9.2.0",
    "nock": "13.2.2",
    "nyc": "15.1.0",
    "shortid": "2.2.16",
    "temp-dir": "2.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "oclif": {
    "commands": "./src/commands",
    "bin": "spaship",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
