{
  "name": "@spaship/manager",
  "version": "0.14.0",
  "description": "The SPAship UI.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:spaship/spaship.git",
    "directory": "packages/manager"
  },
  "author": "Kun Yan <kyan@redhat.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "[ -f ../../.env ] && source ../../.env; HTTPS=true PORT=$SPASHIP_MANAGER_PORT HOST=$SPASHIP_MANAGER_HOST react-scripts start",
    "build": "react-scripts build"
  },
  "devDependencies": {
    "@patternfly/react-core": "4.157.3",
    "@patternfly/react-icons": "4.11.17",
    "@patternfly/react-styles": "4.11.16",
    "@patternfly/react-table": "4.50.1",
    "@react-keycloak/web": "3.4.0",
    "@types/jest": "27.4.0",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.21",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.21",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "keycloak-js": "10.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "react-toastify": "8.1.0",
    "styled-components": "5.3.3",
    "typescript": "4.5.5"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/no-anonymous-default-export": [
        2,
        {
          "allowArrowFunction": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
